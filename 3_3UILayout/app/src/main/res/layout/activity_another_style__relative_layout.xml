<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button_AN_Style_Relative"
        android:layout_toLeftOf="@+id/button_AN_Style_Relative"
        android:textAllCaps="false"
        android:text="Another Style"
        android:id="@+id/button_ThirdLayout_1"
        />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button_AN_Style_Relative"
        android:layout_toRightOf="@+id/button_AN_Style_Relative"
        android:text="button2"
        android:textAllCaps="false"
        android:id="@+id/button_ThirdLayout_2"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button_AN_Style_Relative"
        android:layout_alignStart="@+id/button_ThirdLayout_1"
        android:text="Button3"
        android:textAllCaps="false"
        android:id="@+id/button_ThirdLayout_3"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button_AN_Style_Relative"
        android:layout_alignStart="@+id/button_AN_Style_Relative"
        android:text="button4"
        android:id="@+id/button_ThirdLayout_4"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:text="buTTon5"
        android:textAllCaps="false"
        android:id="@+id/button_AN_Style_Relative"/>

</RelativeLayout>
